}
<link rel="stylesheet" href="~/assets/css/teamTable.css" />
<div class="mo card p-3 mt-3 mx-3">
    <div class="container-fluid">
        <div class="row p-2">
            <div class="table-responsive">
                <table id="employeeTable" class="table table-bordered">
                    <thead>
                        <tr class="text-start">
                            <th>EMP.ID</th>
